<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>JC-apigee-datadog-handler</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <Name>RF-Default-Response</Name>
            </Step>
        </Response>
    </PostFlow>
    <Flows>
</Flows>
    <HTTPProxyConnection>
        <BasePath>/datadog-logs</BasePath>
        <Properties/>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="NoRoute" />
</ProxyEndpoint>
